package lecture5;
import java.util.Scanner;
public class Whileloopprintingno {

	public static void main(String[] args) {
	Scanner s = new Scanner(System.in);
   
    int n = 0;
    while(n<11)
    {
    	System.out.println(n);
    	n++;
    }

	}

}


